*
	list-style none
	margin 0
	padding 0
	outline 0
a
	text-decoration none
body
	background #666 url(../images/sidebar-bg.png) repeat-y 0 0
	color #444
	font 13px/1.7em "Open Sans", "trebuchet ms", arial, sans-serif
h1
h2
h3
h4
h5
h6	
p
	margin 0 auto 10px
	font-family 'Open Sans', 'trebuchet ms', arial, sans-serif
h3
	font-size 24px
// button
.btn
	border 1px solid transparent
	border-radius 4px
	cursor pointer
	display inline-block
	font-size 14px
	font-weight normal
	line-height 1.428571429
	margin-bottom 0
	padding 6px 12px
	text-align center
	vertical-align middle
	white-space nowrap
	-webkit-user-select none
	-moz-user-select none
	-ms-user-select none
	-o-user-select none
	user-select none
.btn-default
	background-color #fff
	border-color #ccc
	color #333
.btn-primary
	background-color #e5412d
	border-color #dd301b
	color #ffffff
.btn-block
	display block
	padding-right 0
	padding-left 0
	width 100%
	
// input
input[type=text]
input[type=password]
	background-color #fff
	border 1px solid #ccc
	border-radius 4px
	box-shadow inset 0 1px 1px rgba(0,0,0,0.075)
	color #555
	display block
	font-size 14px
	height 34px
	line-height 1.428571429
	padding 6px 12px
	transition border-color ease-in-out .15s,box-shadow ease-in-out .15s
	vertical-align middle
	-webkit-box-shadow inset 0 1px 1px rgba(0,0,0,0.075)
	-webkit-transition border-color ease-in-out .15s,box-shadow ease-in-out .15s
	width 95%
#wrapper
	width 100%
#site-logo
	left 15px
	position absolute
	top 15px
	z-index 21
#header
	background #222
	background-image -webkit-gradient(linear, left 0%, left 100%, from(#3f3f3f), to(#222222))
	background-image -webkit-linear-gradient(top, #3f3f3f, 0%, #222222, 100%)
	background-image -moz-linear-gradient(top, #3f3f3f 0%, #222222 100%)
	background-image linear-gradient(to bottom, #3f3f3f 0%, #222222 100%)
	background-repeat repeat-x
	border-bottom 1px solid #000
	height 135px
	position relative
	width 100%
	z-index 15
.caret
	border-top 4px solid #000000
	border-right 4px solid transparent
	border-bottom 0 dotted
	border-left 4px solid transparent
	content ""
 	display inline-block
	height 0
	margin-left 2px
	vertical-align middle
	width 0
// TOP BAR
#top-bar
	color #fff
	display block
	position absolute
	right 25px
	top 0
	z-index 20
	ul
		display block
		float right
		> li
			display block
			float left
			font-size 12px
			position relative
			> a
				color #fff
				display block
				line-height 20px
				padding 8px 10px
				position relative
				&:hover
					background-color #444
				i
					margin-right 5px
				.caret
					border-bottom-color #fff
					border-top-color #fff
					display inline-block
					margin-left 5px
			ul
				background-color #fff
				background-clip padding-box
				border 1px solid rgba(0,0,0,0.15)
				border-radius 0 0 4px 4px
				display none
				float left
				font-size 14px
				left 0
				margin 0
				min-width 160px
				padding 5px 0
				position absolute
				top 35px
				z-index 1000
				-webkit-box-shadow 0 6px 12px rgba(0,0,0,0.175)
				&:before
					content ''
					display inline-block
					border-left 7px solid transparent
					border-right 7px solid transparent
					border-bottom 7px solid #fff
					position absolute
					top -7px
					right 9px
				> li
					display block
					padding 0 7px
					width 200px
					> a
						border-radius 4px
						display block
						clear both
						color #333
						padding 6px 12px
						white-space nowrap
						&:hover
							color #fff
							background-color #e5412d
			&:hover
				ul
					display block
	.divider
		background-color #e5e5e5
		height 1px
		margin 9px 0
		overflow hidden
// SIDE BAR
#sidebar-wrapper
	display block
	#sidebar
		display block
		float left
		width 240px
		position absolute
		z-index 16
		hr
			border-bottom 1px solid #858585
			border-top 1px solid #4A4A4A
			margin-bottom 2em
		#main-nav
			border-bottom 1px solid #858585
			border-top 1px solid #4A4A4A
			margin 1.25em 0 2.5em
			padding 0
			position relative
			top -17px
			width 240px
			& > li
				border-bottom 1px solid #4A4A4A
				border-top 1px solid #858585
				list-style-type none
				margin 0
				padding 0
				position relative
				& > a
					-moz-box-sizing border-box
					-webkit-box-sizing border-box
					box-sizing border-box
					color #fff
					display inline-block
					font-size 13px
					font-weight normal
					outline none
					padding 11px 18px 11px 18px
					text-decoration none
					text-shadow 1px 1px 2px rgba(0, 0, 0, 0.7)
					width 100%
					i
						color #444
						display inline-block
						font-size 16px
						margin-right 1em
						position relative
						text-align center
						text-shadow none
						top 1px
						width 16px
					&:hover
						background #616161
						background rgba(0, 0, 0, 0.075)
				&.active
					border-top none
					& > a
						background #066ECD
						background-image -webkit-gradient(linear, left 0%, left 100%, from(#ffffff), to(#f0f0f0))
						background-image -webkit-linear-gradient(top, #ffffff, 0%, #f0f0f0, 100%)
						background-image -moz-linear-gradient(top, #ffffff 0%, #f0f0f0 100%)
						background-image linear-gradient(to bottom, #ffffff 0%, #f0f0f0 100%)
						background-repeat repeat-x
						border-top none
						box-shadow inset 0 1px 0 rgba(255, 255, 255, 0.3), 0 1px 1px rgba(0, 0, 0, 0.075)
						color #e5412d
						font-weight 600
						text-shadow none
						i
							color #e5412d
				&.dropdown
					& > a
						padding-bottom 12px
						.caret
							border-bottom-color #383838
							border-top-color #383838
							display inline-block
							height 0
							position absolute
							right 20px
							top 19px
							width 0
			.sub-nav
				background #555
				border-bottom none
				display none
				list-style-type none
				margin-bottom 0
				margin-top 0
				padding 1em 0
				padding-left 25px
				position relative
				top 0
				width 100%
				& > li
					background none
					list-style-type none
					margin 0 1em 0 0
					padding 0
					& > a
						color #FFF
						display block
						font-size 12px
						padding 7px 15px 7px 30px
						width auto
						&:hover
							border-radius 5px
							background-color #4d4d4d
							text-decoration none
						i
							color #fff
							font-size 13px
							margin-right .65em
							position relative
							top 1px
#content
	background #FFF
	left auto
	margin-left 240px
	margin-right 20px
	min-height 800px
	padding-bottom 25px
	position relative
	top auto
	width auto
	z-index 18
	#content-header
		background-image -webkit-gradient(linear, left 0%, left 100%, from(#ffffff), to(#eeeeee))
		background-image -webkit-linear-gradient(top, #ffffff, 0%, #eeeeee, 100%)
		background-image -moz-linear-gradient(top, #ffffff 0%, #eeeeee 100%)
		background-image linear-gradient(to bottom, #ffffff 0%, #eeeeee 100%)
		background-repeat repeat-x
		border-bottom 1px solid #CCC
		border-top-right-radius 4px
		border-top-left-radius 4px
		height 96px
		margin-bottom 2em
		position relative
		top -96px
		width 100%
		&:before
		&:after
			content " "
			display table
		&:after
			clear both
		h1
			color #444
			font-size 32px
			font-weight 400
			left 20px
			letter-spacing -2px
			line-height 96px
			margin 0
			position relative
			text-shadow 1px 1px 2px rgba(255, 255, 255, 0.5)
	#content-container
		padding 0 20px
		position relative
		top -96px
		.portlet
			margin-bottom 2em
		.row
			margin-bottom 2em
			& > div .portlet:last-child
				margin-bottom 0
.col-md-3
	width 25%
.col-md-1, .col-md-2, .col-md-3, .col-md-4, .col-md-5, .col-md-6, .col-md-7, .col-md-8, .col-md-9, .col-md-10, .col-md-11
	float: left;
.col-xs-1, .col-xs-2, .col-xs-3, .col-xs-4, .col-xs-5, .col-xs-6, .col-xs-7, .col-xs-8, .col-xs-9, .col-xs-10, .col-xs-11, .col-xs-12, .col-sm-1, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-sm-10, .col-sm-11, .col-sm-12, .col-md-1, .col-md-2, .col-md-3, .col-md-4, .col-md-5, .col-md-6, .col-md-7, .col-md-8, .col-md-9, .col-md-10, .col-md-11, .col-md-12, .col-lg-1, .col-lg-2, .col-lg-3, .col-lg-4, .col-lg-5, .col-lg-6, .col-lg-7, .col-lg-8, .col-lg-9, .col-lg-10, .col-lg-11, .col-lg-12
	position relative
	min-height 1px
	padding-right 15px
	padding-left 15px
.row
	box-sizing border-box
	color rgb(68, 68, 68)
	display block
	font-family 'Open Sans', 'trebuchet ms', arial, sans-serif
	font-size 13px
	font-style normal
	font-variant normal
	font-weight normal
	height 240px
	line-height 22.1000003814697px
	margin 0 -15px 26px -15px
	width 1007px
	&:before
	&:after
		content ''
		display table
	&:after
		clear both
.portlet
	clear both
	position relative
	width auto
	.portlet-header
		-webkit-background-clip padding-box
		background-image -webkit-gradient(linear, left 0%, left 100%, from(#fafafa), to(#ebebeb))
		background-image -webkit-linear-gradient(top, #fafafa, 0%, #ebebeb, 100%)
		background-image -moz-linear-gradient(top, #fafafa 0%, #ebebeb 100%)
		background-image linear-gradient(to bottom, #fafafa 0%, #ebebeb 100%)
		background-repeat repeat-x
		border 1px solid #D5D5D5
		border-top-right-radius 4px
		border-top-left-radius 4px
		position relative
		padding 1px 15px
		&:before
		&:after
			content " "
			display table

		&:after
			clear both
		h1
		h2
		h3
		h4
		h5
		h6 
			color #555
			display inline-block
			float left
			font-size 15px
			font-weight 600
			margin 0 1em 0 0
			padding 10px 0
			text-shadow 1px 1px 2px rgba(255, 255, 255, 0.5)
			vertical-align middle
			i
				color #777
				display inline-block
				font-size 16px
				margin-top -3px
				margin-right 10px
				vertical-align middle
	.portlet-tools
		display inline-block
		float left
		margin 5px 0 0 0
		padding 0
		& > li
			display inline-block
			list-style none
			margin 0
			padding 0
			vertical-align middle
		.btn-icon
			color #555
			font-size 16px
			margin-top -2px
			&:hover
				background-color #fff
				text-decoration none

			.caret
				margin-top -3px
		.dropdown-menu
			left auto
			right 0
		.btn.btn-sm
			margin-bottom 0
			padding-bottom 3px
			padding-top 3px
		.form-control.input-sm
			display inline-block
			height 26px
			width 125px
	.portlet-content
		background #FFF
		background-clip padding-box
		border 1px solid #D5D5D5
		border-radius 4px
		padding 22px 15px 15px
		-webkit-border-radius 4px
		-webkit-background-clip padding-box
		-moz-border-radius 4px
		-moz-background-clip padding
		&:after
		&:before
			content ''
			display table
			
	
























